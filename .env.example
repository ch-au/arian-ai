# Python Negotiation Service Logging
# Control log verbosity for the negotiation engine
# Options: DEBUG, INFO, WARNING, ERROR
# INFO: Shows high-level workflow (rounds, outcomes) - recommended for production
# DEBUG: Shows detailed internal operations - use for troubleshooting
PYTHON_LOG_LEVEL=INFO

# Negotiation Model Configuration
# Override model selection from Langfuse prompts
# NEGOTIATION_MODEL=gemini/gemini-flash-lite-latest
# LITELLM_MODEL=gpt-4o-mini

# Langfuse Configuration
LANGFUSE_SECRET_KEY=your_secret_key
LANGFUSE_PUBLIC_KEY=your_public_key
LANGFUSE_HOST=https://cloud.langfuse.com

# Database
DATABASE_URL=your_database_url

# Neon Auth Configuration
VITE_STACK_PROJECT_ID=your_neon_auth_project_id
VITE_STACK_PUBLISHABLE_CLIENT_KEY=your_neon_auth_publishable_key
STACK_SECRET_SERVER_KEY=your_neon_auth_secret_key
