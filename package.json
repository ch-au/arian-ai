  "scripts": {
    "dev:server": "NODE_ENV=development PYTHON_LOG_LEVEL=INFO tsx server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:debug": "NODE_ENV=development PYTHON_LOG_LEVEL=DEBUG concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "build": "npm run build:frontend && npm run build:backend && npm run build:copy-python",
    "build:frontend": "vite build",
    "build:backend": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --external:./vite-dev",
    "build:copy-python": "node scripts/copy-python.js",
    "start": "NODE_ENV=production node dist/index.js",
    "postinstall": "echo 'Postinstall completed.'",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx server/seed.ts",
    "test": "vitest",
    "prompt:smoke": "tsx scripts/prompt-smoke.ts",
    "prepare": "husky"
  },
